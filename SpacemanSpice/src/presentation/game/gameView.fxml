<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.canvas.Canvas?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ProgressBar?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="720.0" prefWidth="1280.0" stylesheets="@gameViewStyle.css" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="presentation.game.GameViewController">
   <children>
      <Canvas fx:id="canvasMap" height="720.0" width="1280.0" />
      <VBox prefHeight="720.0" prefWidth="1280.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <HBox prefHeight="50.0" prefWidth="600.0" spacing="40.0">
               <children>
                  <VBox prefHeight="200.0" prefWidth="100.0">
                     <children>
                        <ProgressBar fx:id="progressBarLife" prefHeight="38.0" prefWidth="100.0" progress="0.0">
                           <VBox.margin>
                              <Insets bottom="5.0" />
                           </VBox.margin>
                        </ProgressBar>
                        <ProgressBar fx:id="progressBarOxygen" prefHeight="38.0" prefWidth="100.0" progress="0.0">
                           <VBox.margin>
                              <Insets />
                           </VBox.margin>
                        </ProgressBar>
                     </children>
                  </VBox>
                  <VBox prefHeight="50.0" prefWidth="249.0">
                     <children>
                        <HBox prefHeight="100.0" prefWidth="133.0" spacing="10.0">
                           <children>
                              <Label text="Wave:" />
                              <Label fx:id="waveNumberValue" text="waveNumber_Value" />
                           </children>
                        </HBox>
                        <HBox prefHeight="100.0" prefWidth="200.0" spacing="10.0">
                           <children>
                              <Label text="Time:" />
                              <Label fx:id="waveTimeLabel" text="waveTime_Value" />
                           </children>
                        </HBox>
                     </children>
                  </VBox>
               </children>
               <VBox.margin>
                  <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
               </VBox.margin>
            </HBox>
            <Pane prefHeight="369.0" prefWidth="1280.0" />
            <HBox alignment="BOTTOM_LEFT" prefHeight="261.0" prefWidth="1280.0" spacing="700.0">
               <children>
                  <VBox alignment="BOTTOM_LEFT" minWidth="500.0" prefHeight="198.0" prefWidth="500.0">
                     <children>
                        <HBox prefHeight="202.0" prefWidth="500.0">
                           <children>
                              <TextArea fx:id="outputText" editable="false" maxWidth="100.0" minWidth="100.0" prefHeight="194.0" prefWidth="430.0" />
                              <TextArea fx:id="infoText" editable="false" focusTraversable="false" prefHeight="100.0" prefWidth="400.0" wrapText="true">
                                 <font>
                                    <Font name="Consolas" size="12.0" />
                                 </font>
                              </TextArea>
                           </children>
                        </HBox>
                        <TextField fx:id="inputText" onKeyPressed="#enterPressedHandler" />
                     </children>
                     <HBox.margin>
                        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
                     </HBox.margin>
                  </VBox>
               </children>
               <VBox.margin>
                  <Insets />
               </VBox.margin>
            </HBox>
         </children>
      </VBox>
   </children>
</AnchorPane>
